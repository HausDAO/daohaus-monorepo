{
  "root": "apps/haus-price",
  "projectType": "application",
  "sourceRoot": "apps/haus-price/src",
  "targets": {
    "build-base": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/haus-price",
        "main": "apps/haus-price/src/main.ts",
        "tsConfig": "apps/haus-price/tsconfig.app.json",
        "externalDependencies": "all"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "externalDependencies": "none"
        }
      }
    },
    "serve": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "offline",
        "buildTarget": "haus-price:build-base"
      }
    },
    "build": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": ["apps/haus-price/.serverless", "dist/apps/haus-price"],
      "options": {
        "command": "package",
        "buildTarget": "haus-price:build-base:production"
      }
    },
    "deploy": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": ["apps/haus-price/.serverless", "dist/apps/haus-price"],
      "options": {
        "command": "deploy",
        "buildTarget": "haus-price:build-base:production"
      }
    },
    "remove": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "remove"
      }
    },
    "sls": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {}
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/haus-price"],
      "options": {
        "jestConfig": "apps/haus-price/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/haus-price/src/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
